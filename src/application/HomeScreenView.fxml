<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="250.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SceneController">
   <children>
      <BorderPane prefHeight="92.0" prefWidth="531.0">
         <center>
            <Label alignment="CENTER" prefHeight="53.0" prefWidth="800.0" text="Welcome to the Fashion Program" textAlignment="CENTER" BorderPane.alignment="CENTER">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
      <BorderPane prefHeight="81.0" prefWidth="531.0">
         <center>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="800.0" text="Pick an action to begin:" BorderPane.alignment="CENTER">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
      <GridPane alignment="CENTER" prefHeight="66.0" prefWidth="800.0">
        <columnConstraints>
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#switchToClothingChoiceScene" prefHeight="25.0" prefWidth="200.0" text="Size Calculator">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Laundry Pal" GridPane.columnIndex="1">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
            <Button alignment="CENTER" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Outfit Creator" GridPane.columnIndex="2">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Button>
         </children>
      </GridPane>
      <BorderPane prefHeight="82.0" prefWidth="400.0">
         <center>
            <Button mnemonicParsing="false" onAction="#switchToScene2" prefHeight="41.0" prefWidth="125.0" text="Settings" BorderPane.alignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </center>
      </BorderPane>
   </children>
</VBox>
